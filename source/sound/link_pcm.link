MEMORY {
	ram: org = 0x40000, len = 0x40000
}

SECTIONS {
	code: {
		*(signature)
		. = 0x40010;
		*(code)
	} > ram
	data: {
		*(data_sfx_index)
		*(data_song_priorities)
		*(data_song_priorities_end)
		*(data_sfx_priorities)
		*(data_sfx_priorities_end)
		*(data_cmd_priorities)
		*(data_cmd_priorities_end)
		*(data_song_index)
		*(data_songs)
		*(data_sfx)
		*(data_sample_index)
		*(data_sample_index_end)
		*(data_samples)
	} > ram
}
